{
  "name": "angular-folder-structure",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run tailwind &&  ng serve",
    "build": "npm run tailwind-prod && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tailwind": "npx tailwindcss-cli@latest build ./src/assets/styles/tailwindcss/tailwind.css -c ./tailwind.config.js -o ./src/assets/styles/tailwindcss/tailwind-build.css",
    "tailwind-prod": "NODE_ENV=production npx tailwindcss-cli@latest build ./src/assets/styles/tailwindcss/tailwind.css -c ./tailwind.config.js -o ./src/assets/styles/tailwindcss/tailwind-build.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
